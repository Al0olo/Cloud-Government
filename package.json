{
    "name": "city-portal-backend",
    "version": "1.0.0",
    "description": "City Portal Backend Application",
    "main": "dist/server.js",
    "scripts": {
        "start": "node dist/server.js",
        "dev": "nodemon",
        "build": "tsc",
        "migrate": "ts-node -r tsconfig-paths/register src/scripts/migrate.ts",
        "migrate:reset": "ts-node -r tsconfig-paths/register src/scripts/migrate.ts --reset",
        "seed": "ts-node -r tsconfig-paths/register src/scripts/migrate.ts --seed",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "aws-sdk": "^2.1048.0",
        "bcrypt": "^5.0.1",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.17.2",
        "express-rate-limit": "^6.2.0",
        "joi": "^17.6.0",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "multer": "^1.4.4",
        "nodemailer": "^6.9.16",
        "pg": "^8.7.1",
        "winston": "^3.5.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/joi": "^17.2.3",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.7",
        "@types/node": "^17.0.10",
        "@types/nodemailer": "^6.4.16",
        "@types/pg": "^8.6.4",
        "@types/uuid": "^10.0.0",
        "nodemon": "^2.0.15",
        "ts-node": "^10.4.0",
        "tsconfig-paths": "^3.12.0",
        "typescript": "^4.5.4"
    }
}
